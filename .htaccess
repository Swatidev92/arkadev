DirectoryIndex index.php
RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*) /index.php?q=$1&%{QUERY_STRING} [L]

RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
RewriteRule ^uploaded_files/?$ - [F,L]
RewriteRule ^uploaded_files/proposal/?$ - [F,L]
RewriteRule ^uploaded_files/smtpmail/?$ - [F,L]
RewriteRule ^uploaded_files/inc2/?$ - [F,L]
RewriteRule ^uploaded_files/lib/?$ - [F,L]
RewriteRule ^uploaded_files/inc/?$ - [F,L]
RewriteRule ^uploaded_files/assets/?$ - [F,L]



